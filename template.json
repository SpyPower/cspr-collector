{
  "index_patterns": [
    "cspr-violations-*",
    "cspr-violations"
  ],
  "settings": {
    "number_of_shards": 1,
    "number_of_replicas": 1
  },
  "mappings": {
    "_doc": {
      "properties": {
        "document-uri": {
          "type": "keyword"
        },
        "referrer": {
          "type": "keyword"
        },
        "violated-directive": {
          "type": "keyword"
        },
        "effective-directive": {
          "type": "keyword"
        },
        "original-policy": {
          "type": "keyword"
        },
        "disposition": {
          "type": "keyword"
        },
        "blocked-uri": {
          "type": "keyword"
        },
        "status-code": {
          "type": "integer"
        },
        "script-sample": {
          "type": "keyword"
        },
        "occurred": {
          "type": "date"
        }
      }
    }
  }
}
